---

<Layout Title="AI Automation Consulting | GIRIRAJ DIGITAL">

    <!-- * Hero Section -->
    <HeroSection Padding={true} BackImage={HeroBg} SubTitle="AI Automation" , Title="Revolunize your AI Services: With Giriraj Digital as your Partner!" />

    <!-- * Contact Now -->
    <LeftSideCard RightImage={RightImage} SubTitle="Overview" Title="Making work easy for AI Automation Company" Paragraph={ParagraphContent} />

    <!-- * Driving Success: Our Impact on AI Companies  -->
    <section class="section-container grid grid-cols-12 gap-[30px]">
        <div class="col-span-12 lg:col-start-2 lg:col-span-10 space-y-10 ">
            <SectionTitle Title="Driving Success: Our Impact on AI Companies" Heading="The Number Game:"
            SpanBlock="How We Have Helped AI Companies?"   />

            <div class="grid grid-cols-1 md:grid-cols-2 xl:grid-cols-4 gap-[30px]">
                <PlansCard Icon={Icon1} Title="10+ AI Automation Clients in Our Bag" Border={false} Padding={false} Bgcolor="#E9F2FC" />
                <PlansCard Icon={Icon2} Title="2 of the 10 Clients Onboarded" Border={false} Padding={false} Bgcolor="#E9F2FC" />
                <PlansCard Icon={Icon3} Title="500+ Users Within Just the First Month" Border={false} Padding={false} Bgcolor="#E9F2FC" />
                <PlansCard Icon={Icon4} Title="Client User Retention Average 68% to 91%" Border={false} Padding={false} Bgcolor="#E9F2FC" />
            </div>
        </div>
    </section>

    <TechStack />

    <ServiceOffered />

    <!-- * Our Clients -->
    <section class="section-container grid grid-cols-12 gap-[30px]">
        <div class="col-span-12 lg:col-start-2 lg:col-span-10 space-y-[30px] lg:space-y-[60px]">
            <SectionTitle Title="Our Clients" Heading="The customers who have joined hands with Giriraj Digital for their AI development and experienced soaring success!" WidthClass="lg:w-[70%]" />

            <div class="grid grid-cols-2 md:grid-cols-3 xl:grid-cols-4 gap-5 lg:gap-[30px]">
                <ClientImageCard Image={Stripe} AltText="Stripe" />
                <ClientImageCard Image={burrow} AltText="burrow" />
                <ClientImageCard Image={theSill} AltText="theSill" />
                <ClientImageCard Image={huckberry} AltText="huckberry" />
                <ClientImageCard Image={github} AltText="github" />
                <ClientImageCard Image={shopify} AltText="shopify" />
                <ClientImageCard Image={food} AltText="food" />
                <ClientImageCard Image={microsoft} AltText="microsoft" />
            </div>
        </div>
    </section>

    <!-- * Contact Now -->
    <ContactNowHeading Heading="Are you ready to level up your AI business with us? Book a free consultation right away!", Image={ContactBg}, isImageHidden={true} />

    <!-- * SwipeUp Card -->
    <SwipeUpCard CardsData={cards}  Title="Embracing Modernity" Heading="The Modern Methodology for Modern Businesses" Paragraph="Modern businesses want greater speed, efficiency, adaptability, and efficiency. Giriraj Digital's team has the necessary expertise and experience to provide all of that to AI firms via specialized engineering teams and services. Using the greatest tools and platforms available, we automate and streamline procedures while providing creative solutions." />

    <!-- * Our Process Blueprint  -->
    <section class="section-container grid grid-cols-12 gap-5 lg:gap-[30px] space-y-[30px] lg:space-y-[60px]">
        <div class="col-span-12 lg:col-start-2 lg:col-span-8">
            <SectionTitle Title="Our Process Blueprint" Heading="Process we Follow to archive best result" />
        </div>

        <div class="col-span-12 lg:col-start-2 lg:col-span-10  grid grid-cols-1 md:grid-cols-2 xl:grid-cols-3 gap-[30px] lg:gap-10">
            <TextCard Icon={ImageIcon1} Title="knowledgeable AI Experts", Paragraph={KnowMoreCards} />
            <TextCard Icon={ImageIcon2} Title="Dynamic Expertise", Paragraph={DynamicExperts} />
            <TextCard Icon={ImageIcon3} Title="Cost-Effective Solutions", Paragraph={CostEffectiveness} />

            <TextCard Icon={ImageIcon4} Title="Communication at the Core", Paragraph={CommunicationCore} />
            <TextCard Icon={ImageIcon5} Title="Embracing the (constant) Change", Paragraph={EmbracingChange} />
            <TextCard Icon={ImageIcon6} Title="Holistic Project Management", Paragraph={HolisticProject} />

            <TextCard Icon={ImageIcon7} Title="Enhanced Emphasis on Scalability", Paragraph={EnhancedEmphasis} />
            <TextCard Icon={ImageIcon8} Title="Commitment to Data Security and Compliance", Paragraph={CommitmentSecurity} />
        </div>
    </section>

    <!-- * Testimonial -->
    <Testimonial />

    <!-- * Solution Delievered -->
    <SolutionDelivered />

    <!-- * Featured Resources -->
    <FeatureResources />

    <!-- * FAQs -->
    <FaqContainer FaqList={faqList}  />

    <!-- * Contact Now -->
    <ContactUsForm ContactUsBgImage={ContactUsBg} />
</Layout>