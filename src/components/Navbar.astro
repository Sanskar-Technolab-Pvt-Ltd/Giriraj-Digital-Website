              <!-- Content -->
              <span class="relative z-10 flex items-center"> <p class="font-semibold text-[#2C363A] group-hover:text-white transition-colors duration-500"> See our Awards & Recognition </p>
                <img src={Arrow2.src} alt="arrow" class="block group-hover:hidden pl-2.5" />
                <img src={Arrow.src} alt="arrow" class="hidden group-hover:block pl-2.5" />
              </span>
            </a>

          </div>

          <div class="space-y-8">
