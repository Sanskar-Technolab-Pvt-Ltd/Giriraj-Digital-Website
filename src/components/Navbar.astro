            <a href="/" class="relative group h-fit w-fit rounded-full border border-[#D0DDE2] flex items-center py-2 lg:py-3 px-3.5 lg:px-6 font-semibold overflow-hidden cursor-pointer">
              <!-- Gradient overlay -->
              <span class="absolute inset-0 rounded-full bg-[linear-gradient(90deg,#0FA1DB,#5C2D91)] opacity-0 group-hover:opacity-100 transition-opacity duration-500 ease-in-out"></span>

              <!-- Content -->
              <span class="relative z-10 flex items-center"> <p class="font-semibold text-[#2C363A] group-hover:text-white transition-colors duration-500"> About our Leadership Team </p>
                <img src={Arrow2.src} alt="arrow" class="block group-hover:hidden pl-2.5" />
                <img src={Arrow.src} alt="arrow" class="hidden group-hover:block pl-2.5" />
              </span>
